# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Sentry Error Tracking (Optional - for production monitoring)
# Get your DSN from: https://sentry.io/settings/projects/your-project/keys/
VITE_SENTRY_DSN=https://your-dsn-here.ingest.sentry.io/your-project-id

# App Configuration
VITE_APP_VERSION=1.0.0

# Analytics (Optional)
VITE_ANALYTICS_ENDPOINT=

# Feature Flags
VITE_ENABLE_AUDIT=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
