    "@types/react-router-dom": "^5.3.3",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "framer-motion": "^12.23.24",
    "hoist-non-react-statics": "^3.3.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
