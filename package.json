{
  "name": "accounting-system",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "clean": "node -e \"const fs = require('fs'); if(fs.existsSync('dist')) fs.rmSync('dist', {recursive: true, force: true});\"",
    "dev": "vite",
    "dev:test": "vite --mode testing",
    "build": "vite build",
    "build:test": "vite build --mode testing",
    "build:analyze": "tsc -b && vite build --mode analyze",
    "lint": "eslint .",
    "preview": "npm run build && vite preview . --host --port 4173 --strictPort",
    "preview:test": "npm run build:test && vite preview . --host --port 4174 --strictPort",
    "check:migration": "tsx scripts/checkLineItemsMigration.ts",
    "test": "vitest run",
    "test:performance": "node scripts/performance-test.js",
    "deploy:optimized": "bash scripts/deploy-optimized.sh",
    "performance:enable": "echo 'true' > .performance-monitoring",
    "performance:disable": "rm -f .performance-monitoring",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:install": "playwright install",
    "help:screenshots": "tsx scripts/generateHelpScreenshots.ts"
  },
  "dependencies": {
